.content{
  margin-top: 60rpx;
  justify-content: space-around;
}
.card{
  width: 40%;
  height: 280rpx;
  border: 1px solid hsl(0, 0%, 88%);
  border-radius: 30rpx;
}
.card-image{
  width: 100%;
  height: 200rpx;
  border-radius: 30rpx;
}
.text{
  font-size: 14px;
  text-align: center;
  color: gray;
  margin-top: 10rpx;
}