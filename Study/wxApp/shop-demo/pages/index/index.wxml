<swiper class="swiper" indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circle="{{circle}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image"/>
    </swiper-item>
  </block>
</swiper>

<view class="content">
  <view class="props" wx:for="{{propsList}}" wx:key="{{index}}">
    <image class="bed-image" src="../../images/content.jpg" />
    <view class="name">{{item.name}}</view>
    <view class="price">ï¿¥{{item.price}}</view>
  </view>

 
</view>